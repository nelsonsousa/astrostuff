[connection]
id=astrostuff-hotspot
uuid=ae4f45df-ac56-459b-8609-2b1fbd1ea82d
type=wifi
autoconnect=true
interface-name=wlan1

[wifi]
mode=ap
ssid=${ASTROSTUFF_HOTSPOT_SSID}
hidden=false

[wifi-security]
key-mgmt=wpa-psk
psk=${ASTROSTUFF_HOTSPOT_PASSWORD}

[ipv4]
method=shared

[ipv6]
addr-gen-mode=default
method=auto

[proxy]
